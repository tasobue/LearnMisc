Function insertData(log As Variant, position As Integer)
    
    Dim d As Long
    Dim name As String
        
        'd = Cells(Rows.Count, 1).End(xlUp).Row + 2
        
        
        For Each lists In log
        
            'Cells(a, "D").Value = "i"
            'Cells(7 + i, "F").Value = "a"
            'Cells(7 + i, "H").Value = "n"
        
            
            
            
            
            If InStr(lists, "LogOn") > 0 Then
            
                'セルが空の場合
                '変数にTrueFalseを持たせる
                If Cells(position, "E").Value = "" Then
                
                '時間だけを取得する
                name = Mid(lists, 10, 4)
                
                Cells(position, "E").Value = name
             
                
                End If
                
                'ElseIf InStr(lists, "ScreenLock") > 0 Then
                
                'Cells(d, "A").Interior.Color = RGB(0, 0, 255)
                
                
               ' ElseIf InStr(lists, "ScreenUnlock") > 0 Then
                
                'Cells(d, "A").Interior.Color = RGB(0, 255, 0)
                
            
            ElseIf InStr(lists, "LogOff") > 0 Then
            
                name = Mid(lists, 10, 4)
            
                Cells(position, "G") = name
                
                
            Else
            
            End If
            
            d = d + 1
            
            Set wb = Nothing
            Set ws = Nothing
                
        Next lists

    Close #2
    
End Function
